      </cts:json-property-scope-query>
      <cts:element-query>
        <cts:element xmlns:es="http://marklogic.com/entity-services">es:instance</cts:element>
        <cts:true-query />
      </cts:element-query>
    </cts:or-query>
  </search:additional-query>
