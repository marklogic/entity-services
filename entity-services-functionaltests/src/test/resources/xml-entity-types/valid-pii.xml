<?xml version="1.0" encoding="UTF-8"?>
<es:model xmlns:es="http://marklogic.com/entity-services">
  <es:info>
    <es:title>PIITest</es:title>
    <es:version>0.0.1</es:version>
    <es:base-uri>http://marklogic.com/pii</es:base-uri>
  </es:info>
  <es:definitions>
    <Customer>
      <es:properties>
        <CustomerID>
          <es:datatype>string</es:datatype>
        </CustomerID>
        <CompanyName>
          <es:datatype>string</es:datatype>
        </CompanyName>
        <Country>
          <es:datatype>string</es:datatype>
        </Country>
        <ContactName>
          <es:datatype>string</es:datatype>
        </ContactName>
        <ContactTitle>
          <es:datatype>string</es:datatype>
        </ContactTitle>
        <Phone>
          <es:datatype>string</es:datatype>
        </Phone>
      </es:properties>
      <es:namespace>http://marklogic.com/customer</es:namespace>
      <es:namespace-prefix>cust</es:namespace-prefix>
      <es:primary-key>CustomerID</es:primary-key>
      <es:pii>Country</es:pii>
      <es:pii>Phone</es:pii>
      <es:range-index>CustomerID</es:range-index>
      <es:range-index>CompanyName</es:range-index>
      <es:word-lexicon>CustomerID</es:word-lexicon>
      <es:word-lexicon>CompanyName</es:word-lexicon>
    </Customer>
    <Product>
      <es:properties>
        <ProductID>
          <es:datatype>integer</es:datatype>
        </ProductID>
        <ProductName>
          <es:datatype>string</es:datatype>
        </ProductName>
        <UnitPrice>
          <es:datatype>double</es:datatype>
        </UnitPrice>
        <SupplierID>
          <es:datatype>integer</es:datatype>
        </SupplierID>
      </es:properties>
      <es:primary-key>ProductID</es:primary-key>
      <es:required>ProductID</es:required>
      <es:pii>SupplierID</es:pii>
      <es:pii>UnitPrice</es:pii>
      <es:range-index>ProductID</es:range-index>
    </Product>
    <Order>
      <es:properties>
        <OrderID>
          <es:datatype>integer</es:datatype>
        </OrderID>
        <hasCustomerID>
          <es:ref>#/definitions/Customer</es:ref>
        </hasCustomerID>
        <OrderDate>
          <es:datatype>dateTime</es:datatype>
        </OrderDate>
        <ShippedDate>
          <es:datatype>dateTime</es:datatype>
        </ShippedDate>
        <ShipAddress>
          <es:datatype>string</es:datatype>
        </ShipAddress>
        <OrderDetails>
          <es:datatype>array</es:datatype>
          <es:items>
            <es:ref>#/definitions/OrderDetail</es:ref>
          </es:items>
        </OrderDetails>
      </es:properties>
      <es:primary-key>OrderID</es:primary-key>
      <es:pii>hasCustomerID</es:pii>
      <es:pii>OrderDetails</es:pii>
      <es:pii>OrderDate</es:pii>
    </Order>
    <OrderDetail>
      <es:properties>
        <hasProductID>
          <es:ref>#/definitions/Product</es:ref>
        </hasProductID>
        <hasUnitPrice>
          <es:datatype>double</es:datatype>
        </hasUnitPrice>
        <Quantity>
          <es:datatype>integer</es:datatype>
        </Quantity>
      </es:properties>
    </OrderDetail>
    <Superstore>
      <es:properties>
        <OrdID>
          <es:datatype>integer</es:datatype>
        </OrdID>
        <CustID>
          <es:datatype>string</es:datatype>
        </CustID>
        <OrdDate>
          <es:datatype>dateTime</es:datatype>
        </OrdDate>
        <Shipped-Date>
          <es:datatype>dateTime</es:datatype>
        </Shipped-Date>
        <Product-Name>
          <es:datatype>string</es:datatype>
        </Product-Name>
        <Unit-Price>
          <es:datatype>double</es:datatype>
        </Unit-Price>
        <Quant>
          <es:datatype>integer</es:datatype>
        </Quant>
        <Discount>
          <es:datatype>string</es:datatype>
        </Discount>
        <Ship-Address>
          <es:datatype>array</es:datatype>
          <es:items>
            <es:ref>#/definitions/ShipDetails</es:ref>
          </es:items>
        </Ship-Address>
      </es:properties>
      <es:primary-key>OrdID</es:primary-key>
      <es:pii>OrdID</es:pii>
      <es:pii>OrdDate</es:pii>
      <es:pii>Ship-Address</es:pii>
      <es:range-index>OrdID</es:range-index>
      <es:range-index>CustID</es:range-index>
      <es:range-index>Product-Name</es:range-index>
      <es:range-index>Quant</es:range-index>
    </Superstore>
    <ShipDetails>
      <es:properties>
        <Province>
          <es:datatype>string</es:datatype>
        </Province>
        <Region>
          <es:datatype>string</es:datatype>
        </Region>
        <ShipMode>
          <es:datatype>string</es:datatype>
        </ShipMode>
        <ShippingCost>
          <es:datatype>double</es:datatype>
        </ShippingCost>
      </es:properties>
    </ShipDetails>
  </es:definitions>
</es:model>