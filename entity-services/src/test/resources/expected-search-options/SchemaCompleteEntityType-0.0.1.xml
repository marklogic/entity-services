<search:options xmlns:search="http://marklogic.com/appservices/search">
  <search:constraint name="entity-type">
    <search:value>
      <search:element name="title" ns="http://marklogic.com/entity-services"/>
    </search:value>
  </search:constraint>
  <search:constraint name="orderId">
    <search:value>
      <search:element name="orderId" ns=""/>
    </search:value>
  </search:constraint>
  <search:constraint name="arrayKey">
    <search:range facet="true" type="xs:long">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/arrayKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="dateKey">
    <search:range facet="true" type="xs:date">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/dateKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="referenceInThisFile">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/referenceInThisFile</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="arrayreferenceInThisFile">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/arrayreferenceInThisFile</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="externalReference">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/externalReference</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="externalArrayReference">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/externalArrayReference</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="byteKey">
    <search:range facet="true" type="xs:int">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/byteKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="shortKey">
    <search:range facet="true" type="xs:int">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/shortKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="unsignedByteKey">
    <search:range facet="true" type="xs:unsignedInt">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/unsignedByteKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="unsignedShortKey">
    <search:range facet="true" type="xs:unsignedInt">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/unsignedShortKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="stringKey">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/stringKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="iriKey">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/iriKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="anyURIKey">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/anyURIKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="booleanKey">
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/booleanKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="integerKey">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/integerKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="negativeIntegerKey">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/negativeIntegerKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="nonNegativeIntegerKey">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/nonNegativeIntegerKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="positiveIntegerKey">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/positiveIntegerKey</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="nonPositiveIntegerKey">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/nonPositiveIntegerKey</search:path-index>
    </search:range>
  </search:constraint>
  <!--This item is a duplicate and is commented out so as to create a valid artifact.
 <search:constraint name="stringKey" xmlns:search="http://marklogic.com/appservices/search">
  <search:word>
    <search:element ns="" name="stringKey"/>
  </search:word>
</search:constraint> 
-->
  <search:constraint name="quantity">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:ex="http://example.org/order-details-namespace" xmlns:es="http://marklogic.com/entity-services">//es:instance/ex:OrderDetails/ex:quantity</search:path-index>
    </search:range>
  </search:constraint>
  <search:constraint name="productName">
    <search:range facet="true" collation="http://marklogic.com/collation/codepoint" type="xs:string">
      <search:element ns="http://example.org/order-details-namespace" name="productName" xmlns:es="http://marklogic.com/entity-services"/>
    </search:range>
  </search:constraint>
  <search:tuples name="SchemaCompleteEntityType">
    <search:range facet="true" type="xs:long">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/arrayKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:date">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/dateKey</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/referenceInThisFile</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/arrayreferenceInThisFile</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/externalReference</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/externalArrayReference</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:int">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/byteKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:int">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/shortKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:unsignedInt">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/unsignedByteKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:unsignedInt">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/unsignedShortKey</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/stringKey</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/iriKey</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/anyURIKey</search:path-index>
    </search:range>
    <search:range collation="http://marklogic.com/collation/en" facet="true" type="xs:string">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/booleanKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/integerKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/negativeIntegerKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/nonNegativeIntegerKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/positiveIntegerKey</search:path-index>
    </search:range>
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/SchemaCompleteEntityType/nonPositiveIntegerKey</search:path-index>
    </search:range>
  </search:tuples>
  <search:tuples name="OrderDetails">
    <search:range facet="true" type="xs:decimal">
      <search:path-index xmlns:es="http://marklogic.com/entity-services">//es:instance/ex:OrderDetails/ex:quantity</search:path-index>
    </search:range>
    <search:range facet="true" collation="http://marklogic.com/collation/codepoint" type="xs:string">
      <search:element xmlns:es="http://marklogic.com/entity-services" name="productName" ns="http://example.org/order-details-namespace"/>
    </search:range>
  </search:tuples>
  <!--Uncomment to return no results for a blank search, rather than the default of all results
 <search:term xmlns:search="http://marklogic.com/appservices/search">
  <search:empty apply="no-results"/>
</search:term> 
-->
  <search:values name="uris">
    <search:uri/>
  </search:values>
  <!--Change to 'filtered' to exclude false-positives in certain searches-->
  <search:search-option>unfiltered</search:search-option>
  <!--Modify document extraction to change results returned-->
  <search:extract-document-data selected="include">
    <search:extract-path xmlns:ex="http://example.org/order-details-namespace" xmlns:es="http://marklogic.com/entity-services">//es:instance/(SchemaCompleteEntityType|ex:OrderDetails)</search:extract-path>
  </search:extract-document-data>
  <!--Change or remove this additional-query to broaden search beyond entity instance documents-->
  <search:additional-query>
    <cts:element-query xmlns:cts="http://marklogic.com/cts">
      <cts:element xmlns:es="http://marklogic.com/entity-services">es:instance</cts:element>
      <cts:true-query/>
    </cts:element-query>
  </search:additional-query>
  <!--To return facets, change this option to 'true' and edit constraints-->
  <search:return-facets>false</search:return-facets>
  <!--To return snippets, comment out or remove this option-->
  <search:transform-results apply="empty-snippet"/>
</search:options>
